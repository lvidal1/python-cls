{
  "name": "challenge",
  "repository": "https://github.com/clearsummit/challenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "test": "jest -u --config ./jest.config.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "storybook": "start-storybook -p 6006 -c ./.storybook",
    "coverage": "jest --coverage",
    "test:watch": "NODE_ENV=development yarn test --watch",
    "start:watch": "NODE_ENV=development webpack --progress --colors --watch --config webpack.config.js",
    "start": "NODE_ENV=development webpack-dev-server --progress --colors --watch --config webpack.config.js",
    "start:dev": "NODE_ENV=development LOCAL_API=false node_modules/.bin/webpack-dev-server --progress --colors --watch --config webpack.config.js",
    "production-build": "NODE_ENV=production webpack --config webpack.deploy.js",
    "staging-build": "NODE_ENV=staging webpack --config webpack.staging.js",
    "build-storybook": "build-storybook",
    "build-storybook:screenshot": "SCREENSHOT=true build-storybook",
    "screenshot": "SCREENSHOT=true storybook-chrome-screenshot -p 9001 -c .storybook",
    "loki": "loki --reactUri file:./storybook-static",
    "loki:update": "loki update --reactUri file:./storybook-static",
    "regression": "reg-suit run",
    "prettier:fix": "prettier --write './**/*.{scss,json,js,jsx}' --config ../.prettierrc",
    "prettier": "prettier --list-different './**/*.{scss,json,js,jsx}' --config ../.prettierrc",
    "sass-lint": "sass-lint ./**/**/*.scss -v -q --config ../.sass-lint.yml",
    "sass-lint:fix": "sass-lint-auto-fix --config ../.sass-lint.yml"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.10.4",
    "@challenge/models": "^0.0.1",
    "@clearsummit/carabiners": "^0.0.2",
    "@clearsummit/external-frame": "^0.1.0",
    "@clearsummit/radio-dispatch": "0.6.13",
    "@storybook/addon-a11y": "6.0.21",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-knobs": "6.0.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addon-storyshots": "6.0.21",
    "@storybook/addon-viewport": "6.0.21",
    "@storybook/core": "6.0.21",
    "@storybook/react": "6.0.21",
    "@storybook/theming": "6.0.21",
    "@types/axios": "^0.14.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "^0.29.3",
    "autoprefixer": "^9.4.7",
    "axios": "^0.18.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "@babel/preset-react": "^7.0.0",
    "connected-react-router": "^6.2.2",
    "css-loader": "^4.3.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "express": "^4.16.4",
    "formik": "^2.2.6",
    "heroku-ssl-redirect": "^0.0.4",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "loki": "^0.14.2",
    "moment": "^2.22.2",
    "node-sass": "^4.9.4",
    "normalize.css": "^8.0.1",
    "nyc": "^13.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.12",
    "react-inlinesvg": "^0.8.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-saga": "^1.1.3",
    "reg-keygen-git-hash-plugin": "^0.7.16",
    "reg-notify-github-plugin": "^0.7.19",
    "reg-notify-slack-plugin": "^0.7.16",
    "reg-publish-s3-plugin": "^0.7.18",
    "reg-suit": "^0.7.17",
    "regenerator-runtime": "^0.12.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^1.3.4",
    "storybook-chrome-screenshot": "^1.4.0",
    "style-loader": "^2.0.0",
    "timekeeper": "^2.2.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "webpack": "^5.0.0-rc.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "codeclimate-test-reporter": "^0.5.1",
    "enzyme": "^3.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-styled-components": "^6.3.1",
    "react-svg-loader": "^3.0.3",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.15.0",
    "storybook-react-router": "^1.0.8",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^2.5.2"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      },
      "chrome.ipad": {
        "target": "chrome.app",
        "width": 960,
        "height": 640,
        "deviceScaleFactor": 1,
        "mobile": true
      }
    }
  }
}
